{
  "name": "maigexr-desktop",
  "productName": "maigeXR",
  "version": "1.0.0",
  "description": "maigeXR - AI-powered Extended Reality development platform for desktop",
  "private": true,
  "main": "electron/dist/main.js",
  "author": {
    "name": "maigeXR Team",
    "email": "hello@maigexr.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "dev:electron": "concurrently -k \"cross-env NODE_ENV=development next dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "build": "next build",
    "build:static": "cross-env ELECTRON_BUILD=true next build",
    "build:electron": "pnpm run build:static && pnpm run electron:compile && electron-builder",
    "build:electron:win": "pnpm run build:static && pnpm run electron:compile && electron-builder --win",
    "build:electron:mac": "pnpm run build:static && pnpm run electron:compile && electron-builder --mac",
    "build:electron:linux": "pnpm run build:static && pnpm run electron:compile && electron-builder --linux",
    "build:electron:all": "pnpm run build:static && pnpm run electron:compile && electron-builder -mwl",
    "electron:compile": "tsc -p tsconfig.electron.json",
    "electron:dev": "cross-env NODE_ENV=development electron .",
    "electron:start": "cross-env NODE_ENV=production electron .",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "type-check:electron": "tsc -p tsconfig.electron.json --noEmit",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@codesandbox/sandpack-client": "^2.13.5",
    "@codesandbox/sandpack-react": "^2.13.5",
    "@monaco-editor/react": "^4.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.7.3",
    "framer-motion": "^10.16.16",
    "jszip": "^3.10.1",
    "lucide-react": "^0.294.0",
    "monaco-editor": "^0.45.0",
    "next": "14.0.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "sql.js": "^1.13.0",
    "tailwind-merge": "^2.1.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@types/sql.js": "^1.4.9",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "next-pwa": "^5.6.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.0",
    "wait-on": "^7.2.0"
  },
  "optionalDependencies": {
    "better-sqlite3": "^9.2.0",
    "keytar": "^7.9.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "build": {
    "extends": "electron-builder.yml"
  }
}
